<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/muveran"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#B0C4DE"
    tools:context=".exo.ui.ExpeditionActivity">
    <include layout="@layout/toolbar"/>


    <ImageView
            android:id="@+id/imageMuveran"
            android:layout_width="173dp"
            android:layout_height="161dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="13dp"
            android:layout_marginTop="98dp"
            app:srcCompat="@drawable/muverans" />


    <TextView
        android:id="@+id/titreMuveran"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="106dp"
        android:layout_marginEnd="18dp"
        android:text="Tour des Muverans"
        android:textColor="#F2FFFFFF"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/lieuxMuv"
        android:layout_width="157dp"
        android:layout_height="21dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="568dp"
        android:text="@string/LieuxMuv"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/DistanceMuv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="78dp"
        android:layout_marginBottom="543dp"
        android:text="@string/DistanceMuv"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/DifficulteMuv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="67dp"
        android:layout_marginBottom="517dp"
        android:text="@string/DifficultéMuv"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/PrixMuv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginEnd="123dp"
        android:layout_marginBottom="492dp"
        android:text="@string/PrixMuv"
        android:textColor="@color/white" />

    <ImageView
        android:id="@+id/imageWalser"
        android:layout_width="180dp"
        android:layout_height="186dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="14dp"
        android:layout_marginBottom="277dp"
        app:srcCompat="@drawable/walser" />

    <TextView
        android:id="@+id/titreWalser"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="289dp"
        android:layout_marginEnd="67dp"
        android:text="Tour Walser"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/lieuxWals"
        android:layout_width="153dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="383dp"
        android:text="@string/LieuxWals"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/DistanceWals"
        android:layout_width="129dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_centerVertical="true"
        android:layout_marginEnd="71dp"
        android:layout_marginBottom="356dp"
        android:text="@string/DistanceWals"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/DifficulteWals"
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_centerVertical="true"
        android:layout_marginEnd="58dp"
        android:layout_marginBottom="332dp"
        android:text="@string/DifficultéWals"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/PrixWals"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginEnd="125dp"
        android:layout_marginBottom="308dp"
        android:text="@string/PrixWals"
        android:textColor="@color/white" />

    <ImageView
            android:id="@+id/image7lacs"
            android:layout_width="171dp"
            android:layout_height="172dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="11dp"
            android:layout_marginBottom="120dp"
            app:srcCompat="@drawable/lacs" />

    <TextView
        android:id="@+id/titre7lacs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="77dp"
        android:layout_marginBottom="262dp"
        android:text="Les 7 lacs"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/lieux7lacs"
        android:layout_width="153dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="37dp"
        android:layout_marginBottom="238dp"
        android:text="@string/Lieux7lacs"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/Distance7lacs"
        android:layout_width="129dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="61dp"
        android:layout_marginBottom="212dp"
        android:text="@string/Distance7lacs"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/Difficulte7lacs"
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_centerVertical="true"
        android:layout_marginEnd="52dp"
        android:layout_marginBottom="187dp"
        android:text="@string/Difficulté7lacs"
        android:textColor="@color/white" />


    <TextView
        android:id="@+id/Prix7lacs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginEnd="113dp"
        android:layout_marginBottom="162dp"
        android:text="@string/Prix7lacs"
        android:textColor="@color/white" />

    <ImageButton
        android:id="@+id/ExpeditionAdd"
        android:layout_width="63dp"
        android:layout_height="66dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="142dp"
        android:layout_marginBottom="63dp"
        android:background="@null"
        app:srcCompat="@android:drawable/ic_input_add" />

    <ImageButton
        android:id="@+id/imageButtonEdit"
        android:layout_width="63dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="150dp"
        android:layout_marginBottom="80dp"
        android:background="@null"
        app:srcCompat="@android:drawable/ic_menu_edit" />


</RelativeLayout>